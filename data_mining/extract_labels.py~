import csv

def extract_labels(csvfile, label, delimeter=",", quotechar="\""):
    labels = set()
    csvdict = csv.DictReader(csvfile, delimeter=delimeter,  quotechar=quotechar)
    for d in csvdict:
        labels.add(d[labell])
    return labels

